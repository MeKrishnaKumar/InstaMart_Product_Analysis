                        `}else{const onclickAttr=link.getAttribute('onclick')||'';const iconClass=link.className.split(' ').find(cls=>cls.startsWith('h_ic'))||'';subItem.innerHTML=`
                            <span onclick="${onclickAttr}" 
                                  class="hhdr-submenu-link ${iconClass}" 
                                  style="cursor: pointer;">
                                ${link.textContent}
                            </span>
                        `}
submenu.appendChild(subItem)})}
menuItemsContainer.appendChild(item)}}}
function toggleMenu(forceClose=!1){const dropdown=document.getElementById('hhdr-menu-panel');const hamburger=document.getElementById('hhdr-hamburger-btn');if(!dropdown||!hamburger)return;const isOpen=dropdown.classList.contains('active');if(isOpen||forceClose){dropdown.classList.remove('active');hamburger.classList.remove('active');document.body.classList.remove('mobile-menu-open');removeBackdrop()}else{populateMenuItems();dropdown.classList.add('active');hamburger.classList.add('active');document.body.classList.add('mobile-menu-open');createBackdrop()}}
function setupEventListeners(){document.addEventListener('click',function(e){const hamburger=document.getElementById('hhdr-hamburger-btn');const dropdown=document.getElementById('hhdr-menu-panel');if(!hamburger||!dropdown)return;if(hamburger.contains(e.target)){e.preventDefault();e.stopPropagation();toggleMenu();return}
if(e.target.classList.contains('hhdr-close-btn')){e.preventDefault();e.stopPropagation();toggleMenu(!0);return}
if(e.target.classList.contains('dropdown-toggle')){e.preventDefault();e.stopPropagation();const menuItem=e.target.closest('.hhdr-nav-item');if(menuItem){menuItem.classList.toggle('expanded')}
return}
const menuLink=e.target.closest('.hhdr-nav-link, .hhdr-submenu-link');if(menuLink&&dropdown.contains(menuLink)){const isActionLink=menuLink.hasAttribute('data-action');if(isActionLink){setTimeout(()=>{if(typeof isPopupOpen==='function'&&isPopupOpen()){toggleMenu(!0)}else{console.log(' Action executed - keeping menu open')}},300);return}
setTimeout(()=>{toggleMenu(!0)},100);return}});document.addEventListener('keydown',function(e){if(e.key==='Escape'){const dropdown=document.getElementById('hhdr-menu-panel');if(dropdown&&dropdown.classList.contains('active')){toggleMenu(!0)}}})}
function handleResize(){const hamburger=document.getElementById('hhdr-hamburger-btn');const dropdown=document.getElementById('hhdr-menu-panel');if(isMobile()){if(!hamburger)createHamburgerButton();if(!dropdown)createDropdownMenu();}else{if(hamburger)hamburger.remove();if(dropdown)dropdown.remove();removeBackdrop();document.body.classList.remove('mobile-menu-open')}}
function init(){if(!isMobile()){return}
createHamburgerButton();createDropdownMenu();populateMenuItems();setupEventListeners()}
if(document.readyState==='loading'){document.addEventListener('DOMContentLoaded',init)}else{init()}
window.addEventListener('resize',handleResize)})()